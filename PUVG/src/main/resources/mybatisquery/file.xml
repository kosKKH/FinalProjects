<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
          
<mapper namespace="main.puvg.fileupload.dao.FileuploadDAO">

	<select id="fileuploadSelectALL" resultType="filevo">
	SELECT * FROM FILEVO
	</select>
	
	<insert id="fileuploadInsert" parameterType="filevo">
	INSERT INTO FILEVO(
	     MESSAGE
	    ,FILENAME
	)
	VALUES(
	     #{message, jdbcType=VARCHAR}	
	    ,#{filename, jdbcType=VARCHAR}
	)
	</insert>
	

</mapper>

